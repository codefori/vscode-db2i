        "title": "Advised Indexes and Statistics",
        "category": "Db2 for i",
        "icon": "$(lightbulb)"
      },
      {
        "command": "vscode-db2i.dove.editSettings",
        "title": "Settings",
        "category": "Db2 for i",
        "icon": "$(gear)"
      },
      {
        "command": "vscode-db2i.dove.export",
        "title": "Export current VE data",
        "category": "Db2 for i",
        "icon": "$(file)"
